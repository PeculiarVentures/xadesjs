{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-classes-per-file": false,
        "max-line-length": false,
        "unified-signatures": false,
        "no-namespace": false,
        "object-literal-sort-keys": false,
        "interface-name": false,
        "prefer-for-of": false,
        "forin": false,
        "no-conditional-assignment": false,
        "no-var-requires": false
    },
    "rulesDirectory": []
}